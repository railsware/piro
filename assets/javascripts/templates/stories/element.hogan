<div class="story {{current_state}}">
  <a href="#" class="story_link_info story-title">
    <div>
      <span class="{{story_type}}-icon"></span>
    </div>
    {{name}}
  </a>
  {{#labelsList}}
    <a href="#" class="story_label label">{{.}}</a>
  {{/labelsList}}
  {{#owned_by}}
    <span class="muted">(</span><a href="#" class="story_owned_by">{{initials}}</a><span class="muted">)</span>
  {{/owned_by}}
</div>